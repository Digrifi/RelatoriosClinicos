@model IEnumerable<SistemaClinicaRelatorio.Domain.DTO.DoctorQueryReportDTO>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.idDoctor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.nameDoctor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.cnpj)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.crm)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.specialty)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.pacient)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.assessment)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.idDoctor)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.nameDoctor)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.cnpj)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.crm)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.specialty)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.pacient)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.date)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.assessment)
                </td>
            </tr>
        }
    </tbody>
</table>